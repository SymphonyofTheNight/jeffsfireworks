{"components/common/social-links":"{{#if social_media}}\n    <ul class=\"socialLinks socialLinks--alt\">\n        {{#each social_media}}\n            <li class=\"socialLinks-item\">\n                {{#if display_name '===' 'google+'}}\n                    <a class=\"icon icon--google\" href=\"{{url}}\" target=\"_blank\">\n                        <svg><use xlink:href=\"#icon-google\" /></svg>\n                    </a>\n                {{else}}\n                    <a class=\"icon icon--{{display_name}}\" href=\"{{url}}\" target=\"_blank\">\n                        <svg><use xlink:href=\"#icon-{{display_name}}\" /></svg>\n                    </a>\n                {{/if}}\n            </li>\n        {{/each}}\n    </ul>\n{{/if}}\n","components/sbthemes/module/sb_newletter_popup/default_items":"\r\n<div id=\"sb-newletter-popup\" class=\"hide\">\r\n    <div class=\"popup-overlay\"></div>\r\n    <div class=\"newsletter-popup-container\">\r\n        <a href=\"#\" class=\"modal-close-newletter\" aria-label=\"{{lang 'common.close'}}\" role=\"button\" data-close-newsletter-popup>\r\n            <span aria-hidden=\"true\">&#215;</span>\r\n        </a>\r\n        <div class=\"newsletter-content d-flex\">\r\n\t\t\t<div class=\"newsletter-popup--left col d-none d-sm-block\">\r\n\t\t\t\t{{#if image '!=' ' '}}\r\n\t\t\t\t\t<img data-sizes=\"auto\" class=\"img-fluid lazyload \" src=\"data:image/gif;base64,R0lGODlhAQABAIAAAP///wAAACH5BAEAAAAALAAAAAABAAEAAAICRAEAOw==\" data-src=\"{{cdn image }}\" alt=\"{{subscribe}}\"/>\r\n\t\t\t\t{{else}}\r\n\t\t\t\t\t<img class=\"img-fluid\" src=\"{{cdn placeholder}}\" alt=\"{{subscribe}}\"/>\r\n\t\t\t\t{{/if}}\r\n\t\t\t</div>\r\n\t\t\t\r\n\t\t\t<div class=\"newsletter-popup--right col\">\r\n\t\t\t\t<h2 class=\"newsletter-heading\">{{subscribe}}</h2>\r\n\t\t\t\t<p class=\"newsletter-sub-heading\">{{subscribe_intro}}</p>\r\n\t\t\t\t<div id=\"popupSubcribeForm\">\r\n\t\t\t\t\t\r\n\t\t\t\t\t<form class=\"form \" action=\"{{urls.subscribe.action}}\" method=\"post\" id=\"popupSubcribeFormSubmit\">\r\n\t\t\t\t\t\t<fieldset class=\"form-fieldset\">\r\n\t\t\t\t\t\t\t<input type=\"hidden\" name=\"action\" value=\"subscribe\">\r\n\t\t\t\t\t\t\t<input type=\"hidden\" name=\"nl_first_name\" value=\"bc\">\r\n\t\t\t\t\t\t\t<input type=\"hidden\" name=\"check\" value=\"1\">\r\n\t\t\t\t\t\t\t<div class=\"form-inline\">\r\n\t\t\t\t\t\t\t\t<div class=\"form-group\">\r\n\t\t\t\t\t\t\t\t\t<input class=\"form-input\" id=\"nl_email\" name=\"nl_email\" type=\"email\" placeholder=\"{{lang 'newsletter.email_placeholder'}}\">\r\n\t\t\t\t\t\t\t\t</div>\r\n\t\t\t\t\t\t\t\t<input class=\"button button--action\" type=\"submit\" value=\"{{lang 'newsletter.subscribe_submit'}}\">\r\n\t\t\t\t\t\t\t</div>\r\n\t\t\t\t\t\t</fieldset>\r\n\t\t\t\t\t</form>\r\n\t\t\t\t\t<label class=\"hidden-popup\">\r\n\t\t\t\t\t\t<input type=\"checkbox\" value=\"1\" name=\"hidden-popup\">\r\n\t\t\t\t\t\t<span class=\"inline\">{{subscribe_showpopup}}</span>\r\n\t\t\t\t\t</label>\r\n\t\t\t\t</div>\r\n\t\t\t\t<div class=\"footer-socials\">\r\n\t\t\t\t\t{{> components/common/social-links}}\r\n\t\t\t\t</div>\r\n\t\t\t\t \r\n\t\t\t</div>\r\n        </div>\r\n    </div>\r\n</div>\r\n\r\n\r\n\r\n\t\n"}
